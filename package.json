{
  "name": "wiretyped",
  "version": "0.2.0",
  "description": "Typed HTTP client with error-first ergonomics, caching, retries, SSE, and runtime validation powered by @standard-schema/spec.",
  "type": "module",
  "keywords": [
    "http-client",
    "api-client",
    "typed",
    "typescript",
    "standard-schema",
    "validation",
    "error-first",
    "fetch",
    "rest",
    "sse",
    "retry",
    "cache"
  ],
  "author": "Kasper Rynning-TÃ¸nnesen <kasperrt@users.noreply.github.com> (https://github.com/kasperrt)",
  "license": "MIT",
  "repository": "https://github.com/kasperrt/wiretyped",
  "packageManager": "pnpm@10.20.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": ["dist"],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/types/core/index.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.cjs"
    },
    "./error": {
      "types": "./dist/types/error/index.d.ts",
      "import": "./dist/error.mjs",
      "require": "./dist/error.cjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build && pnpm run build:types",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly",
    "check": "pnpm check:lint && pnpm check:format && pnpm check:ts",
    "check:format": "pnpm exec biome format --error-on-warnings",
    "check:lint": "pnpm exec biome lint --error-on-warnings",
    "check:ts": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "fix": "pnpm exec biome check --write",
    "fix:format": "pnpm exec biome format --write",
    "fix:lint": "pnpm exec biome lint --write",
    "prepare": "pnpm exec simple-git-hooks",
    "prepublishOnly": "pnpm check && pnpm run test && pnpm run build",
    "smoke:build": "pnpm run build && pnpm run smoke",
    "smoke:bun": "bun run scripts/smoke-bun.ts",
    "smoke:deno": "deno run --allow-read scripts/smoke-deno.ts",
    "smoke:browser": "node scripts/smoke-browser.mjs",
    "smoke:node": "node scripts/smoke-node.mjs",
    "smoke:types": "pnpm exec tsc -p scripts/types-smoke/tsconfig.json",
    "smoke:worker": "node scripts/smoke-worker.mjs",
    "test": "vitest run --no-isolate --no-file-parallelism",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "playwright": "^1.57.0",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.15",
    "zod": "^4.1.13"
  },
  "simple-git-hooks": {
    "pre-commit": "sh scripts/pre-commit.sh"
  }
}
