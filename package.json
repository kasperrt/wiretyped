{
  "name": "wiretyped",
  "version": "0.2.5-rc-3",
  "description": "Universal fetch-based HTTP client with robust error handling, retries, caching, SSE, and Standard Schema validation.",
  "type": "module",
  "keywords": [
    "http-client",
    "api-client",
    "rest-client",
    "fetch",
    "fetch-api",
    "fetch-client",
    "browser",
    "node",
    "bun",
    "deno",
    "workers",
    "edge",
    "isomorphic",
    "universal",
    "retry",
    "cache",
    "sse",
    "error-first",
    "error-handling",
    "typescript",
    "typed",
    "type-safe",
    "standard-schema",
    "schema-validation",
    "validation"
  ],
  "author": "Kasper Rynning-TÃ¸nnesen <kasperrt@users.noreply.github.com> (https://github.com/kasperrt)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasperrt/wiretyped.git"
  },
  "packageManager": "pnpm@10.25.0",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "types": "./dist/types/index.d.mts",
  "files": ["dist"],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    },
    "./core": {
      "import": {
        "types": "./dist/types/core/index.d.mts",
        "default": "./dist/core.mjs"
      },
      "require": {
        "types": "./dist/types/core/index.d.cts",
        "default": "./dist/core.cjs"
      },
      "default": "./dist/core.mjs"
    },
    "./error": {
      "import": {
        "types": "./dist/types/error/index.d.mts",
        "default": "./dist/error.mjs"
      },
      "require": {
        "types": "./dist/types/error/index.d.cts",
        "default": "./dist/error.cjs"
      },
      "default": "./dist/error.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build && pnpm run build:types",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly && node scripts/generate-cts-types.mjs",
    "check": "pnpm check:lint && pnpm check:format && pnpm check:ts",
    "check:format": "pnpm exec biome format --error-on-warnings",
    "check:lint": "pnpm exec biome lint --error-on-warnings",
    "check:ts": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "fix": "pnpm exec biome check --write",
    "fix:format": "pnpm exec biome format --write",
    "fix:lint": "pnpm exec biome lint --write",
    "prepare": "pnpm exec simple-git-hooks",
    "prepublishOnly": "pnpm check && pnpm run test && pnpm run build",
    "smoke:bun": "bun run scripts/smoke-bun.ts",
    "smoke:deno": "deno run --allow-read scripts/smoke-deno.ts",
    "smoke:browser": "node scripts/smoke-browser.mjs",
    "smoke:entrypoints": "node scripts/smoke-entrypoints.mjs",
    "smoke:dts": "node scripts/smoke-dts.mjs",
    "smoke:resolve": "node scripts/smoke-resolve.mjs",
    "smoke:pack": "node scripts/smoke-pack.mjs",
    "smoke:node": "node scripts/smoke-node.mjs",
    "smoke:types": "pnpm exec tsc -p scripts/types-smoke/tsconfig.json",
    "smoke:worker": "node scripts/smoke-worker.mjs",
    "test": "pnpm test:integrations && pnpm test:e2e",
    "test:integrations": "vitest run --no-isolate --no-file-parallelism",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts --no-isolate --no-file-parallelism",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0"
  },
  "devDependencies": {
    "@hono/node-server": "^1.19.7",
    "@biomejs/biome": "^2.3.8",
    "@codecov/vite-plugin": "^1.9.1",
    "@types/node": "^24.10.2",
    "@vitest/coverage-v8": "^4.0.15",
    "eventsource": "^4.1.0",
    "playwright": "^1.57.0",
    "hono": "^4.10.8",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.1",
    "vitest": "^4.0.15",
    "zod": "^4.1.13"
  },
  "simple-git-hooks": {
    "pre-commit": "sh scripts/pre-commit.sh"
  }
}
